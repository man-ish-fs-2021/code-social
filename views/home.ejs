<h1>Code-social/<%= title %> </h1>
<!-- form to get input from the post -->
<% if(locals.user){%>
<section id="feed-posts">
  <h4>Posts</h4>
  <form action="/posts/create" method="POST" id="new-post-form">
    <textarea name="content" cols="30" rows="3"></textarea>
    <input type="submit" value="Post">
  </form>
</section>
<%}%>

     <div id="post-list-container">
       <ul>
         <% for(post of posts){%>
             <%-include("_post");-%>
          <%}%>
        </ul>
      </div>
<section id="user-friends">
  <h4>Friends</h4>
  <% for(var u of all_users){ %>
    
    <p>
      <a href="/users/profile/<%=u.id%>"><%= u.name%></a>
    </p>
    <%}%>
</section>